#include <bits/stdc++.h>
#define FR(i, n) for (int i = 0; i < (n); ++i)
using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef pair<int, int> ii;

int main() {

   ios_base::sync_with_stdio(false);
   cin.tie(NULL);

   vi A = { 1, 1, 1, 2, 2, 2, 3, 3, 4, 5, 7 };

   cout << "A (size = " << A.size() << ") : ";
   for (auto &x : A) cout << setw(3) << x;
   cout << endl << endl;

   for (int i = 0; i <= 10; i++) {
      cout << i << " : ";
      auto er = equal_range(begin(A), end(A), i);
      cout << "[" << er.first - begin(A) << ", " << er.second - begin(A) << "]" << endl;
   }
   cout << endl;
  
   string B = "11122233457";
   cout << "B (len = " << B.length() << ") : " << B << endl;
   for (int i = 0; i <= 10; i++) {
      cout << i << " : ";
      auto er = equal_range(begin(B), end(B), i + '0');
      cout << "[" << er.first - begin(B) << ", " << er.second - begin(B) << "]" << endl;
   }
   cout << endl;
  

   
}
