#include <bits/stdc++.h>
#define FR(i, n) for (int i = 0; i < (n); ++i)
using namespace std;

typedef long long ll;
typedef vector<int> vi;
typedef pair<ll, ll> ii;


vector<ll> P;
bitset<10000010> p;
ll sz;

void sieve(ll m) {
   sz = m + 1;
   p.set();
   p[0] = p[1] = 0;
   for (ll i = 2; i <= sz; i++) {
      if (p[i]) {
	 for (ll j = i * i; j <= sz; j += i)
	    p[j] = 0;
	 P.push_back(i);      
      }
   }
}

bool good(ll p, ii ab) {
   return !(ab.first % p && ab.second % p);
}

int main() {

   ios_base::sync_with_stdio(false);
   cin.tie(NULL);

   sieve(50000LL);
   int n;
   cin >> n;

   vector<ii> A(n);
   for (auto &x : A) cin >> x.first >> x.second;

   for (int i = 0; i < (int)P.size(); i++) {      
      for (int j = 0; j < n; j++) {
	 if (!good(P[i], A[j])) {
	    break;
	 } else if (j == n - 1) {
	    cout << P[i] << endl;
	    return 0;
	 }
      }
   }
   
   cout << -1 << endl;


   
   

}
