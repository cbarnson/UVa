#include <bits/stdc++.h>
using namespace std;

int main() {

   vector<double> p;
   int n;
   while (cin >> n) {

      string s;
      double x, res = 0.0;
      for (int i = 1; i <= n; i++) {
	 cin >> s >> x;
	 res += x * i;
	 // p.push_back(x);
      }
      // descending order, just in case
      // sort(p.rbegin(), p.rend());
      cout << setprecision(4) << fixed << res << endl;
      

   }


   


   return 0;
}
