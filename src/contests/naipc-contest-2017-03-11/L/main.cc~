#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

int main() {

   ll n;
   cin >> n;

   double p = 0.0;

   for (ll i = 0; i < n; i++) {
      double pp = 1.0;
      for (ll j = 0; j <= i; j++) {
	 cout << "pp *= 1 / " << n - j << endl;
	 pp *= (1 / (double)(n - j));
      }
      p += pp;
   }

   cout << p << endl;
   

   
   return 0;
}
