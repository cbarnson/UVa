#!/usr/bin/python

def gcd(a, b):
    return a if (b == 0) else gcd(b, a % b)

def lcm(a, b):
    return a * (b / gcd(a, b))

def main():
    print('-' * 10 + ' LCM(x, y) ' + '-' * 10)
    print('Enter two numbers to compute LCM, control-C to quit')
    while (1):
        a, b = map(int, input().split())
        print('lcm(%d, %d) = %d' % (a, b, lcm(a, b)))

main()

